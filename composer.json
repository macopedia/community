{
    "name": "macopedia/community",
    "type": "typo3-cms-extension",
    "description": "TYPO3 CMS community / social network extension",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "Community"],
    "support": {
        "issues": "https://github.com/macopedia/community/issues"
    },
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.7.0"
    },
    "autoload": {
        "psr-4": {
            "Macopedia\\Community\\": "Classes/"
        }
    },
    "replace": {
        "community": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "community"
        }
    }
}
